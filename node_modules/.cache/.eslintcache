ssName=\"profile__avatar-container\">\n            <div className=\"profile__avatar-overlay\" onClick={this.props.onEditAvatar}></div>\n            <img className=\"profile__avatar\" src={this.context.avatar} alt=\"Аватар пользователя\" />\n          </div>\n          <div className=\"profile__info\">\n            <div className=\"profile__info-line\">\n              <h1 className=\"profile__username\">\n                {this.context.name}\n              </h1>\n              <button className=\"profile__edit-button\" type=\"button\" onClick={this.props.onEditProfile}></button>\n            </div>\n            <div className=\"profile__info-line\">\n              <p className=\"profile__user-bio\">\n                {this.context.about}\n              </p>\n            </div>\n          </div>\n          <button className=\"profile__add-button\" type=\"button\" onClick={this.props.onAddPlace}></button>\n        </section>\n\n        <section className=\"elements\">\n          <ul className=\"elements__grid\">\n            {this.props.cards.map(card => (\n              <Card\n                card={card}\n                key={card._id}\n                onCardClick={this.props.onCardClick}\n                onCardLike={this.props.onCardLike}\n                onCardDelete={this.props.onCardDelete}\n              />\n            ))}\n          </ul>\n        </section>\n      </main>\n    )\n  }\n}\n\nexport default Main","/Users/dmitriyshumeykin/dev/mesto-react/src/components/PopupWithForm.js",["179"],[],"import React from 'react';\n\nclass PopupWithForm extends React.Component {\n  constructor(props) {\n    super(props)\n  }\n\n  render() {\n    return (\n      <section className={`popup popup_type_${this.props.name} ${this.props.isOpen ? 'popup_opened' : ''}`}>\n        <div className=\"popup__container\">\n          <button className=\"popup__close-button\" type=\"button\" onClick={this.props.onClose}></button>\n          <form className=\"popup__form\" onSubmit={this.props.onSubmit} name={`${this.props.name}`}>\n            <fieldset className=\"popup__input-fields\">\n              <h3 className=\"popup__title\">\n                {this.props.title}\n              </h3>\n              {this.props.children}\n              <button className=\"popup__submit-button\" type=\"submit\" name=\"save\">\n                {this.props.buttonText}\n              </button>\n            </fieldset>\n          </form>\n        </div>\n      </section>\n    )  \n  }\n}\n\nexport default PopupWithForm;","/Users/dmitriyshumeykin/dev/mesto-react/src/components/ImagePopup.js",["180"],[],"import React from 'react'\n\nclass ImagePopup extends React.Component {\n  constructor(props) {\n    super(props)\n  }\n\n  handleClick = () => {\n    console.log(this.props)\n  }\n\n  render () {\n    return (\n      <section className={`popup popup_type_picture-zoom ${this.props.card.isOpen ? 'popup_opened' : ''}`}>\n        <div className=\"popup__container popup__container_type_picture-zoom\">\n          <button className=\"popup__close-button\" type=\"button\" onClick={this.props.onClose}></button>\n          <figure className=\"popup__image-container\">\n            <img className=\"popup__image\" src={this.props.card.link} alt={this.props.card.name} />\n            <figcaption className=\"popup__image-caption\">{this.props.card.name}</figcaption>\n          </figure>\n        </div>\n      </section>\n    )\n  }\n}\n\nexport default ImagePopup","/Users/dmitriyshumeykin/dev/mesto-react/src/utils/Api.js",[],[],"/Users/dmitriyshumeykin/dev/mesto-react/src/components/Card.js",["181"],[],"import React from 'react'\nimport { CurrentUserContext } from '../contexts/CurrentUserContext'\n\nclass Card extends React.Component {\n\n  static contextType = CurrentUserContext\n\n  constructor(props) {\n    super(props)\n  }\n\n  isOwn = () => {\n    return this.props.card.owner._id === this.context._id\n  }\n\n  isLiked = () => {\n    return this.props.card.likes.some(i => i._id === this.context._id)\n  }\n\n  handleLikeClick = () => {\n    this.props.onCardLike(this.props.card)\n  }\n\n  handleDeleteClick = () => {\n    this.props.onCardDelete(this.props.card)\n  }\n\n  handleClick = () => {\n    this.props.onCardClick(this.props.card)\n  }\n  \n  render() {\n    return(\n      <li className=\"element\">\n        <img className=\"element__image\" src={this.props.card.link} alt={this.props.card.name} onClick={this.handleClick}/>\n        <button className={`element__delete-button ${this.isOwn() ? 'element__delete-button_visible' : ''}`} type=\"button\" onClick={this.handleDeleteClick}></button>\n        <div className=\"element__bottom-bar\">\n          <h2 className=\"element__title\">{this.props.card.name}</h2>\n          <button className={`element__like-button ${this.isLiked() ? 'element__like-button_active' : ''}`} type=\"button\" onClick={this.handleLikeClick}></button>\n          <span className=\"element__like-counter\">{this.props.card.likes.length}</span>\n        </div>\n      </li>\n    )\n  }\n}\n\nexport default Card","/Users/dmitriyshumeykin/dev/mesto-react/src/contexts/CurrentUserContext.js",[],[],"/Users/dmitriyshumeykin/dev/mesto-react/src/components/EditAvatarPopup.js",[],[],"/Users/dmitriyshumeykin/dev/mesto-react/src/components/AddPlacePopup.js",[],[],"/Users/dmitriyshumeykin/dev/mesto-react/src/components/EditProfilePopup.js",["182"],[],"import React, { useState, useCallback, useEffect, useContext } from 'react'\nimport PopupWithForm from './PopupWithForm'\nimport { CurrentUserContext } from '../contexts/CurrentUserContext'\n\nfunction EditProfilePopup(props) {\n  \n  const currentUser = useContext(CurrentUserContext)\n  \n  const [formValues, setFormValues] = useState({\n    name: '',\n    description: ''\n  })\n\n  const handleChange = useCallback((event) => {\n    const { name, value } = event.target;\n    setFormValues(prevState => ({ ...prevState, [name]: value }))\n  }, [setFormValues])\n  \n  const { name, description } = formValues;\n\n  useEffect(() => {\n    if (!props.isOpen || 'undefined' || '') {\n      setFormValues({\n        name: currentUser.name,\n        description: currentUser.about\n      })\n    }\n  }, [props.isOpen])\n  \n  function handleSubmit(event) {\n    event.preventDefault()\n    \n    props.onUpdateUser({\n      name,\n      about: description\n    }) \n  }\n    \n    return (\n      <PopupWithForm\n        name={props.name}\n        title={props.title}\n        buttonText={props.buttonText}\n        onClose={props.onClose}\n        isOpen={props.isOpen}\n        onSubmit={handleSubmit}>\n        <label className=\"popup__form-field\">\n          <input\n            value={name || ''}\n            onChange={handleChange}\n            name=\"name\"\n            id=\"username-input\"\n            className=\"popup__input-field\"\n            required\n            type=\"text\"\n            placeholder=\"Имя пользователя\"\n            minLength=\"2\"\n            maxLength=\"40\" />\n          <span className=\"popup__input-error username-input-error\"></span>\n        </label>\n        <label className=\"popup__form-field\">\n          <input\n            value={description || ''}\n            onChange={handleChange}\n            name=\"description\"\n            id=\"job-input\"\n            className=\"popup__input-field\"\n            required\n            type=\"text\"\n            placeholder=\"О себе\"\n            minLength=\"2\"\n            maxLength=\"200\" />\n          <span className=\"popup__input-error job-input-error\"></span>\n        </label>\n    </PopupWithForm>\n  )\n}\n\nexport default EditProfilePopup","/Users/dmitriyshumeykin/dev/react-mesto-auth/src/index.js",[],[],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/reportWebVitals.js",[],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/App.js",[],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/ImagePopup.js",["183"],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/Header.js",["184","185"],[],"import React from 'react';\nimport headerLogo from '../images/headerLogo.svg';\nimport { Link, withRouter } from 'react-router-dom'\n\nclass Header extends React.Component {\n  constructor(props) {\n    super(props)\n  }\n\n  switchLink = () => {\n    let link = { location: '', title: '' }\n    const currentLocation = this.props.history.location.pathname\n    if (currentLocation === '/sign-in') {\n      return link = { location: '/sign-up', title: 'Зарегистрироваться' }\n    } else if (currentLocation === '/sign-up') {\n      return link = { location: '/sign-in', title: 'Войти' }\n    }\n  }\n  \n  render() {\n    return (\n      <header className=\"header\">\n        <img className=\"header__logo\" src={headerLogo} alt=\"Логотип сайта\" />\n        <div className=\"site-entrance__link-container\">\n          <p className=\"site-entrance__email\">{this.props.email}</p>\n          <div>\n            {this.props.history.location.pathname === '/'\n            ?\n            <button className=\"site-entrance__logout-button\" onClick={this.props.onLogoutClick}>Выйти</button>\n            :\n            <Link className=\"site-entrance__redirect-link site-entrance__redirect-link_place_header\" to={this.switchLink().location}>{this.switchLink().title}</Link>}\n          </div>\n        </div>\n      </header>\n    );\n  }\n}\n\nexport default withRouter(Header)","/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/EditProfilePopup.js",["186"],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/Main.js",["187"],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/utils/Api.js",[],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/EditAvatarPopup.js",[],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/PopupWithForm.js",["188"],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/Footer.js",[],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/AddPlacePopup.js",[],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/contexts/CurrentUserContext.js",[],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/Card.js",["189"],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/Login.js",[],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/Register.js",["190"],[],"import React from 'react'\n// import Auth from '../utils/Auth'\nimport { auth } from '../utils/Auth'\nimport { Link, withRouter } from 'react-router-dom'\n\nclass Register extends React.Component {\n  constructor(props){\n    super(props)\n    this.state = {\n      email: '',\n      password: ''\n    }\n  }\n\n  handleChange = event => {\n    const { name, value } = event.target\n    this.setState({\n      [name]: value\n    })\n  }\n\n  handleSubmit = event => {\n    event.preventDefault()\n    this.props.onRegister(this.state)\n  }\n\n  render() {\n    return (\n      <div className=\"site-entrance\">\n      <h3 className=\"site-entrance__title\">\n        Регистрация\n      </h3>\n      <form\n        className=\"site-entrance__form\"\n        onSubmit={this.handleSubmit}\n      >\n        <input\n          className=\"site-entrance__input-field\"\n          required\n          id=\"email\"\n          name=\"email\"\n          type=\"email\"\n          value={this.state.email}\n          onChange={this.handleChange}\n          placeholder=\"Email\" />\n        <input\n          className=\"site-entrance__input-field\"\n          required\n          id=\"password\"\n          name=\"password\"\n          type=\"password\"\n          value={this.state.password}\n          onChange={this.handleChange}\n          placeholder=\"Пароль\" />\n        <button\n          className=\"site-entrance__submit-button\"\n          type=\"submit\" >\n            Зарегистрироваться\n        </button>\n        <Link className=\"site-entrance__redirect-link\" to=\"/sign-in\">\n          Уже зарегистрированы? Войти\n        </Link>\n      </form>\n    </div>\n    )\n  }\n}\n\nexport default withRouter(Register)","/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/InfoTooltip.js",["191","192"],[],"import React from 'react'\nimport successIcon from '../images/popup-site-entrance-confirm-icon-success.svg'\nimport failureIcon from '../images/popup-site-entrance-confirm-icon-failure.svg'\n\nclass InfoTooltip extends React.Component {\n\n  constructor(props) {\n    super(props)\n  }\n\n  render() {\n    return(\n      <section className={`popup popup_type_${this.props.name} ${this.props.isOpen ? 'popup_opened' : ''}`}>\n        <div className=\"popup__container\">\n          <button className=\"popup__close-button\" type=\"button\" onClick={this.props.onClose}></button>\n          <figure className=\"popup__site-entrance-confirm-container\">\n            <img className=\"popup__site-entrance-confirm-icon\" src={this.props.authorizationSuccess ? successIcon : failureIcon}/>\n            <figcaption className=\"popup__site-entrance-confirm-message\">{this.props.authorizationSuccess ? `Вы успешно зарегистрировались!` : `Что-то пошло не так`}</figcaption>\n          </figure>\n        </div>\n      </section>\n    )\n  }\n}\n\nexport default InfoTooltip","/Users/dmitriyshumeykin/dev/react-mesto-auth/src/components/ProtectedRoute.js",[],[],"/Users/dmitriyshumeykin/dev/react-mesto-auth/src/utils/Auth.js",[],[],{"ruleId":"193","severity":1,"message":"194","line":9,"column":3,"nodeType":"195","messageId":"196","endLine":12,"endColumn":4},{"ruleId":"193","severity":1,"message":"194","line":4,"column":3,"nodeType":"195","messageId":"196","endLine":6,"endColumn":4},{"ruleId":"193","severity":1,"message":"194","line":4,"column":3,"nodeType":"195","messageId":"196","endLine":6,"endColumn":4},{"ruleId":"193","severity":1,"message":"194","line":8,"column":3,"nodeType":"195","messageId":"196","endLine":10,"endColumn":4},{"ruleId":"197","severity":1,"message":"198","line":28,"column":6,"nodeType":"199